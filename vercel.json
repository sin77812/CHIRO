{
  "functions": {
    "pages/home/index.html": {
      "runtime": "static"
    },
    "pages/portfolio/portfolio.html": {
      "runtime": "static"
    },
    "pages/about/about.html": {
      "runtime": "static"
    },
    "pages/contact/contact.html": {
      "runtime": "static"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/pages/home/index.html"
    },
    {
      "source": "/portfolio",
      "destination": "/pages/portfolio/portfolio.html"
    },
    {
      "source": "/about",
      "destination": "/pages/about/about.html"
    },
    {
      "source": "/contact",
      "destination": "/pages/contact/contact.html"
    }
  ]
}